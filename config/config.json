{
  "polarityIntegrationUuid": "2b002b50-dca5-11ee-b860-53c490e6f005",
  "name": "Wiz",
  "acronym": "WIZ",
  "description": "Search Wiz for Issues and Vulnerabilities by CVEs and Assets by IP Addresses, Domains, and CVEs",
  "entityTypes": ["cve", "IPv4", "domain"],
  "dataTypes": [
    {
      "type": "custom",
      "name": "Hostname",
      "description": "Search devices by hostname's which are between 4 and 64 characters long and consists of the letters A to Z, 0-9, underscore, and dashes.",
      "editable": true,
      "enabled": false,
      "key": "hostname",
      "regex": "\\b[a-zA-Z_0-9\\-]{4,64}\\b"
    },
    "cve",
    "IPv4",
    "domain"
  ],
